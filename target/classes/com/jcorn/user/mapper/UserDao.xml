<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jcorn.user.dao.UserDao">
    <!-- 用户登录方法 -->
    <select id="queryUser" resultType="com.jcorn.user.entity.User">
        select 
        	username as userName,
        	enabled,
        	name_CN as nameCN
        from tb_user
        where 1=1
    </select>
</mapper>